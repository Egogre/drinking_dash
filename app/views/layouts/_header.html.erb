  <header id="page_header" role="banner">
    <nav id="utility">
      <div id="login">
        <% if signed_in? %>
          <li>
            <%= link_to "Users", '#' %>
          </li>
          <li>
            <%= link_to "Profile", current_user %>
          </li>
          <li>
            <%= link_to "Sign Out", signout_path, method: "delete" %>
          </li>
        <% else %>
          <%= render 'sessions/form' %>
        <% end %>
      </div>
    </nav>
  <%= link_to(image_tag("logo.png", alt: "Logo", id: "logo"), root_url, title: "Dashboard") %>
  </header>
<%= render 'layouts/main_nav' %>
